{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["./service"],
    "exclude": []
  },
  "opts": {
    "destination": "./docs/service",
    "recurse": true,
    "readme": "service/README.md",
    "encoding": "utf8"
  },
  "templates": {
    "referenceTitle": "Task Manager Service",
    "disableSort": false,
    "collapse": true,
    "resources": {
        "google": "https://www.google.com/"
    }
  }
}
